@using WebApplication.Models;
@model ServiceResponse<List<GetVehicleDto>>

@{
    ViewBag.Title = "List";
}

<h2> Vehicle List </h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
       <th>
        @Html.DisplayNameFor(model => model.Data[0].id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].RtoNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data[0].fuel)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Data)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RtoNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fuel)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
                @Html.ActionLink("Details", "Details", new { id = item.id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>
